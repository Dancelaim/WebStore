@model IEnumerable<WowCarry.Domain.Entities.SEO>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AdminSEO</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
