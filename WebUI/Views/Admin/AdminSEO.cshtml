            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MetaRobots)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrlKeyWord)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEOImage)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.SEOId }) |
                @Html.ActionLink("Details", "Details", new { id=item.SEOId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.SEOId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
