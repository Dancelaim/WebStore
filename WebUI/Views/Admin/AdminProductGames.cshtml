@model IEnumerable<WowCarry.Domain.Entities.ProductGame>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AdminProductGames</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.GameName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GameShortUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SEO.MetaTagTitle)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GameName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GameShortUrl)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SEO.MetaTagTitle)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ProductGameId }) |
                @Html.ActionLink("Details", "Details", new { id=item.ProductGameId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ProductGameId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
